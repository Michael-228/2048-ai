{
  "compilerOptions": {
    "target": "es6",
    "module": "esNext",
    "lib": ["dom", "es2015", "webworker", "scripthost"],
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowJs": true,
    "strictNullChecks": true
  }
}
