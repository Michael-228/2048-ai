{
  "compilerOptions": {
    "target": "es6",
    "module": "esNext",
    "lib": ["dom", "es2015", "webworker", "scripthost"],
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./browser/",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowJs": true,
    "strictNullChecks": true
  },
  "include": ["./browser/**/*.ts"]
}
